@model List<Model.EF.GopY_KH>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layouts.cshtml";
}


@if (Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <td>Số điện thoại người dùng</td>
                <td>ID khách hàng</td>
                <td>Nội dung góp ý</td>
                <td>Ngày góp ý</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.IdGopY</td>
                    <td>@item.IdUser</td>
                    <td>@item.NoiDung </td>
                    <td>@item.NgayGopY </td>
                   
                </tr>
            }

        </tbody>
    </table>
    <div>

    </div>

}


else
{
    <span class=" alert alert-danger">
        Chưa đặt hàng lần nào
    </span>
}


