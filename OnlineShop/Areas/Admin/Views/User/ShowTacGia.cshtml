@model List<Model.EF.TacGia>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layouts.cshtml";
}


@if (Model.Count() > 0)
{
    <table class="table">
        <thead>
            <tr>
                <td>Mã tác giả</td>
                <td>Tên tác giả</td>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.MaTacGia</td>
                    <td>@item.TenTacGia</td>
                   
                </tr>
            }

        </tbody>
    </table>
    <div>

    </div>

}


else
{
    <span class=" alert alert-danger">
        Chưa đặt hàng lần nào
    </span>
}


